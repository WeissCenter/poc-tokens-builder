{
  "general": {
    "theme-image-path": {
      "value": "../dist/images",
      "type": "path",
      "description": "Use a path relative to the location of the final compiled CSS."
    },
    "theme-show-compile-warnings": {
      "value": "false",
      "type": "boolean",
      "description": "Show Sass warnings when functions and mixins use non-standard tokens."
    },
    "theme-show-notifications": {
      "value": "false",
      "type": "boolean",
      "description": "Show updates and notifications about the design system."
    },
    "theme-namespace": {
      "value": "light",
      "type": "map",
      "description": "Set a namespace for grid and utility classes."
    },
    "theme-prefix-separator": {
      "value": "\\:",
      "type": "string",
      "description": "Set the character that separates responsive and state prefixes from the main class name. The default (“:”) is preceded by two backslashes so that it is properly escaped."
    },
    "theme-layout-grid-use-important": {
      "value": "false",
      "type": "boolean",
      "description": "Output grid classes with !important ."
    },
    "theme-global-border-box-sizing": {
      "value": "true",
      "type": "boolean",
      "description": "When set to true, sets the box-sizing property of all site elements to border-box."
    },
    "theme-focus-color": {
      "value": "blue-40v",
      "type": "color",
      "description": "The color of the focus outline."
    },
    "theme-focus-offset": {
      "value": "0",
      "type": "units",
      "description": "The offset between the element and the focus outline."
    },
    "theme-focus-style": {
      "value": "solid",
      "type": "outline-style",
      "description": "The style of the focus outline."
    },
    "theme-focus-width": {
      "value": "0.5",
      "type": "units",
      "description": "The width of the focus outline."
    },
    "theme-icon-image-size": {
      "value": "2",
      "type": "units",
      "description": "The size of a standard UI icon (like a close box)."
    }
  },
